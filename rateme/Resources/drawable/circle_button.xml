	</item>
	<item android:state_focused="true">
		<shape android:shape="oval">
			<stroke android:width="1dip" android:color="#ffffff"  />
			<solid android:color="#58857e" />
		</shape>
	</item>
	<item>
		<shape android:shape="oval">
			<stroke android:width="1dip" android:color="#ffffff"  />
			<solid android:color="#58857e" />
		</shape>
	</item>
</selector>